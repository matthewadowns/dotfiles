[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[user]
	name = Matthew Downs
	email = mdowns@box.com
[color]
	ui = auto
[core]
  editor = /usr/bin/vim
  excludesFile = ~/.gitignore
[merge]
  tool = vim
[mergetool "vim"]
  cmd = vim --wait $MERGED
[diff]
  tool = cursor
[difftool "cursor"]
  cmd = cursor --wait --diff $LOCAL $REMOTE
[url "ssh://git@github.com/"]
  pushInsteadOf = https://github.com/
[alias]
	co = checkout
	cob = checkout -b
	coo = !git fetch && git checkout
	br = branch
	brd = branch -d
	brD = branch -D
	merged = branch --merged
	st = status
	aa = add -A .
	cm = commit -m
	aacm = !git add -A . && git commit
	aacm = !git add -A . && git commit -m
	cp = cherry-pick
	amend = commit --amend -m
	main = !git checkout main && git pull origin
	po = push origin
	pu = !git push origin `git branch --show-current`
	pom = push origin main
	poh = push origin HEAD
	pogm = !git push origin gh-pages && git checkout main && git pull origin main && git rebase gh-pages && git push origin main && git checkout gh-pages
	pomg = !git push origin main && git checkout gh-pages && git pull origin gh-pages && git rebase main && git push origin gh-pages && git checkout main
	plo = pull origin
	plom = pull origin main
	ploh = pull origin HEAD
	unstage = reset --soft HEAD^
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	f = "!git ls-files | grep -i"
	gr = grep -Ii
	la = "!git config -l | grep alias | cut -c 7-"
    gs = git status
	logall = log --decorate --all --graph --oneline
